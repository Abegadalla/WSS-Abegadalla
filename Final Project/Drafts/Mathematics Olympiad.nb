Cell[TextData[{
 "If any two (e.g. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "\[LongEqual]", "b"}], TraditionalForm]],ExpressionUUID->
  "7fc681bd-c912-4306-b8c9-7ff87445c117"],
 ") or three roots (",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "a", "\[LongEqual]", "b", "\[LongEqual]", "c", "\[LongEqual]", "1"}], 
   TraditionalForm]],ExpressionUUID->"d8d128c9-586c-4daf-9ea6-609274d0a8d2"],
 ") are identical the resultant ",
 StyleBox["vanishes",
  FontSlant->"Italic"],
 ". Clearly, any permutation of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"a", ",", "b", ",", "c"}], "}"}], TraditionalForm]],
  ExpressionUUID->"c84bf178-b14d-459a-b3c9-79a7beeb7eac"],
 " is permissible."
}], "Text",
 CellChangeTimes->{{3.730175131871129*^9, 3.730175237619008*^9}, {
  3.73017836337724*^9, 3.73017838015202*^9}, {3.730178506567937*^9, 
  3.7301785068297997`*^9}},ExpressionUUID->"fc7ca7c9-a57b-469f-845a-\
c8c6177a3f49"],

Cell[TextData[{
 "Without loss of generality, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "c", "\[GreaterEqual]", "b", "\[GreaterEqual]", "a", "\[GreaterEqual]", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "41cc4319-99aa-4e0c-b65f-81c8d0397eb3"],
 ". With ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"b", "-", "a"}], "\[LongEqual]", "\[Delta]"}], TraditionalForm]],
  ExpressionUUID->"0f6ae94c-4b35-4f1e-aa01-d526c615ba2d"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"c", "-", "b"}], "\[LongEqual]", "\[Epsilon]"}], 
   TraditionalForm]],ExpressionUUID->"5214c55c-4e95-479b-9578-276f5b1fefb9"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"c", "-", "a"}], "\[LongEqual]", 
    RowBox[{"\[Epsilon]", "+", "\[Delta]"}]}], TraditionalForm]],
  ExpressionUUID->"685f639a-d587-4f35-874c-aa7ea0359d73"],
 " so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a", "-", "b"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"b", "-", "c"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"c", "-", "a"}], ")"}]}], "\[LongEqual]", 
    RowBox[{"\[Delta]", " ", "\[Epsilon]", " ", 
     RowBox[{"(", 
      RowBox[{"\[Epsilon]", "+", "\[Delta]"}], ")"}]}], "\[GreaterEqual]", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "84d93bda-fb7f-48a0-a962-318ed9a36387"],
 " and, without loss of generality, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "\[Epsilon]", "\[GreaterEqual]", "\[Delta]", "\[GreaterEqual]", "0"}], 
   TraditionalForm]],ExpressionUUID->"35490b1a-7aee-4538-94b3-8e7b32a127fe"],
 ". This expression is ",
 StyleBox["maximized",
  FontSlant->"Italic"],
 " over the ",
 StyleBox["finite",
  FontSlant->"Italic"],
 " region ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "p", "\[GreaterEqual]", "\[Epsilon]", "\[GreaterEqual]", "\[Delta]", 
    "\[GreaterEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "485eba5c-9cfc-44ef-a001-7587f214cf72"],
 " when ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", "\[LongEqual]", "\[Delta]", "\[LongEqual]", "p"}], 
   TraditionalForm]],ExpressionUUID->"1c77ca39-0406-439a-a725-a20ec6b582a3"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7301759087988987`*^9, 3.7301759992721567`*^9}, {
  3.7301760666966343`*^9, 3.730176125292329*^9}, {3.7301762146007566`*^9, 
