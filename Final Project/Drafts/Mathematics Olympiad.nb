Cell[TextData[{
 "Nice solution by Rodolfo Riverol: Assuming they\[CloseCurlyQuote]re all \
incongruent mod 3, we get the contradiction ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FormBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "2"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{"2", "-", "3"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{"3", "-", "1"}], ")"}]}],
     TraditionalForm], "\[LongEqual]", 
    FormBox[
     TemplateBox[{RowBox[{"(", 
         RowBox[{"1", "+", "2", "+", "3"}], ")"}],"3"},
      "Mod"],
     TraditionalForm]}], TraditionalForm]],ExpressionUUID->
  "f2804e1e-bd02-401f-a31f-e6445409c8dc"],
 " \[DoubleLongRightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FormBox["2",
     TraditionalForm], "\[LongEqual]", 
    FormBox[
     FormBox[
      TemplateBox[{"0","3"},
       "Mod"],
      TraditionalForm],
     TraditionalForm]}], TraditionalForm]],ExpressionUUID->
  "486e68df-b670-427f-8e7e-e93bb4b5fb0a"],
 ". Assuming ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    TemplateBox[{"y","3"},
     "Mod"]}], TraditionalForm]],ExpressionUUID->
  "17d31c15-ef08-41bb-a85d-43e785aeb1f0"],
 " we get ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LongEqual]", 
    TemplateBox[{RowBox[{"(", 
        RowBox[{
          RowBox[{"2", " ", "x"}], "+", "z"}], ")"}],"3"},
     "Mod"], "\[LongEqual]", 
    TemplateBox[{RowBox[{"(", 
        RowBox[{"z", "-", "x"}], ")"}],"3"},
     "Mod"]}], TraditionalForm]],ExpressionUUID->
  "abeaa7b9-c874-4873-af8e-22e5284344b2"],
 " \[DoubleLongRightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    TemplateBox[{"x","3"},
     "Mod"]}], TraditionalForm]],ExpressionUUID->
  "def60819-a605-40e2-8ab8-22d22c702bbe"],
 ".\[NonBreakingSpace] So all three are congruent mod 3 \
\[DoubleLongRightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"27", "\[Divides]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "y"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{"z", "-", "x"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{"y", "-", "z"}], ")"}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"4192b1c7-a735-4d2c-b00e-cb36a963a694"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"27", "\[Divides]", 
    RowBox[{"(", 
     RowBox[{"x", "+", "y", "+", "z"}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"c3bdd24c-7253-4b85-8516-866454e9d44e"],
 "."
}], "Text",
