Cell[BoxData[
 RowBox[{
  RowBox[{
   SubscriptBox["\[ScriptCapitalH]", "q_"], "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "2"], "+", 
    SuperscriptBox["y", "2"], "-", 
    RowBox[{"q", " ", "x", " ", "y"}], "-", "q"}], "\[Equal]", 
   "0"}]}]], "Input",
 CellChangeTimes->{{3.7516029974554377`*^9, 3.7516030220275583`*^9}, {
