Cell[TextData[{
 "Let a, b, c be non-negative such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "+", "b", "+", "c"}], "=", "3"}], TraditionalForm]],
  ExpressionUUID->"aaa05661-c885-47f2-92e1-b0046909c2c9"],
 ". Prove that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{RowBox[{
        RowBox[{"(", 
          RowBox[{"a", "-", "b"}], ")"}], " ", 
        RowBox[{"(", 
          RowBox[{"b", "-", "c"}], ")"}], " ", 
        RowBox[{"(", 
          RowBox[{"c", "-", "a"}], ")"}]}]},
     "Abs"], "\[LessEqual]", 
    RowBox[{"3", " ", 
     RowBox[{
      SqrtBox["3"], "/", "2"}]}]}], TraditionalForm]],ExpressionUUID->
  "bf2401a3-46b3-4ed2-bfab-50605ec3efdb"],
 ". Equality occurs when?"
}], "Text",
 CellChangeTimes->{
  3.7301674346173487`*^9, {3.7301678116292353`*^9, 3.730167818941823*^9}, {
   3.7301678504862423`*^9, 
   3.7301678551148987`*^9}},ExpressionUUID->"267fe63b-a564-4d20-a99b-\
0cfa97a26186"],

