Cell[CellGroupData[{

Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "b", ",", "c"}], "}"}], "\[Element]", 
    SuperscriptBox[
     TemplateBox[{},
